<resources xmlns:tools="http://schemas.android.com/tools">


    <style name="NoActionBarTheme" parent="@style/Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:dropDownListViewStyle">@style/DropDownListViewStyle</item>
    </style>


    <style name="DropDownListViewStyle">
        <item name="android:divider">@drawable/line_divider_with_margins</item>
        <item name="android:dividerHeight">1dp</item>
    </style>


    <attr name="amdp_theme_dark" format="boolean">false</attr>


    <style name="amdp_ActionButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">@dimen/amdp_material_button_height</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:focusable">true</item>
    </style>

    <style name="StreamingDataTableSubHeaderAppearance" parent="@android:style/TextAppearance">
        <item name="android:textColor">#fff</item>
        <item name="android:textStyle">bold</item>
    </style>

    <!-- Due to https://issuetracker.google.com/issues/68454482 bug with Android O+, DO NOT set windowIsTranslucent=true -->
    <style name="RoutingActivityTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>

    <style name="StreamingDataTableValueAppearance" parent="@android:style/TextAppearance">
        <item name="android:textColor">#fff</item>
        <item name="android:fontFamily">monospace</item>
    </style>

    <style name="StreamDataTableLabelAppearance" parent="@android:style/TextAppearance">
        <item name="android:textColor">#fff</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="amdp_ActionButton.Text" tools:ignore="NewApi">
        <item name="android:textSize">@dimen/amdp_material_button_textsize</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:gravity">center</item>
        <item name="android:stateListAnimator">@null</item>
        <item name="android:background">@drawable/amdp_material_button_background</item>
        <item name="android:minWidth">@dimen/amdp_material_button_minwidth</item>
        <item name="android:paddingLeft">@dimen/amdp_material_button_textpadding_horizontal</item>
        <item name="android:paddingRight">@dimen/amdp_material_button_textpadding_horizontal</item>
        <item name="android:textColor">@color/amdp_button_color</item>
        <item name="android:textAllCaps">true</item>
    </style>

    <style name="TreeNodeStyleCustom">
        <item name="android:paddingLeft">20dp</item>
    </style>

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="NavigationPathTextStyle">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">@font/roboto_bold</item>
    </style>

    <style name="swipe_text_color">
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">@font/roboto_bold</item>
    </style>


    <style name="IconTextActionButtonTitle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">2dp</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:textSize">30sp</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textStyle">bold|italic</item>
        <item name="android:scrollHorizontally">false</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:lineSpacingExtra">0dp</item>
    </style>


    <style name="IconTextActionButtonDescription">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">3dp</item>
        <item name="android:textSize">15sp</item>
        <item name="android:gravity">center</item>
        <item name="android:ellipsize">end</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:textStyle">bold|italic</item>
        <item name="android:scrollHorizontally">false</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="menu_settings">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_margin">3dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:weightSum">3</item>
    </style>

    <style name="MenuSettingsButton">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_margin">3dp</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="MenuSettingsButton.Resources" parent="MenuSettingsButton">
        <item name="android:layout_height">100dp</item>
    </style>

    <style name="MenuButtonDescription">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">3dp</item>
        <item name="android:textSize">18sp</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:textStyle">bold|italic</item>
        <item name="android:scrollHorizontally">false</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="MenuDescription">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">3dp</item>
        <item name="android:textSize">20sp</item>
        <item name="android:gravity">center</item>
        <item name="android:textStyle">bold|italic</item>
        <item name="android:scrollHorizontally">false</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="hotspot_button_style">
        <item name="android:layout_width">30dp</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:layout_margin">10dp</item>
        <item name="android:textSize">30sp</item>
        <item name="android:gravity">center</item>

    </style>

    <style name="rl_hotspot_button_wrapper">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">5dp</item>
        <item name="android:background">@drawable/background_view_circle_single_black</item>
    </style>

    <style name="TimeTabItem">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">5dp</item>
        <item name="citabTextSize">12sp</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="menu_rl_item">
        <item name="android:layout_height">150dp</item>
        <item name="android:layout_margin">5dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">0dp</item>
        <item name="citabIconWidth">80dp</item>
        <item name="citabTextSideMargin">15dp</item>
        <item name="citabTextGravity">left</item>
        <item name="citabContentAligment">left</item>
    </style>


    <style name="NavigationActionButton">
        <item name="android:background">?android:selectableItemBackground</item>
    </style>

    <style name="TextLabel" parent="TextAppearance.Design.Hint">
        <item name="android:textSize">14sp</item>
    </style>

    <!--Check date showcase warning-->

    <style name="BaseHintAppearance" parent="@android:style/TextAppearance">
        <item name="android:textColor">@color/accent</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="BaseHintAppearance.SecondaryText">
        <item name="android:textColor">@color/secondary_text</item>
    </style>


    <style name="SegmentNavigationButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
        <item name="android:background">?android:attr/selectableItemBackground</item>
        <item name="android:textSize">28sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="AssetInfoTextStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@drawable/bottom_line</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:fontFamily">@font/roboto_medium</item>
    </style>

    <style name="ab_title">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textSize">24sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:fontFamily">@font/roboto_medium</item>
    </style>

    <style name="BackgroundCardView.Transparent">
        <item name="cardUseCompatPadding">false</item>
        <item name="cardElevation">11dp</item>
        <item name="cardCornerRadius">0dp</item>
