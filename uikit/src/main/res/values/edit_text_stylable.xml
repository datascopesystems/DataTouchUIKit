<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="FormEditText">
        <attr name="et_max_text_length" format="integer"/>
        <attr name="et_hint" format="string" />
        <attr name="et_left_unselected_hint" format="string" />
        <attr name="et_icon" format="reference" />
        <attr name="et_mandatory_field" format="boolean" />
        <attr name="et_inputType" format="enum">
            <enum name="text" value="0" />
            <enum name="number" value="1" />
            <enum name="text_multiline" value="2" />
        </attr>
        <attr name="et_editable" format="boolean"/>
        <attr name="et_max_lines" format="integer"/>
    </declare-styleable>

</resources>