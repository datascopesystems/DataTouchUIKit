<?xml version="1.0" encoding="UTF-8"?>


<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="oval">
            <solid android:color="@color/primary_transparent_dark" />
            <!--shadow Color-->
        </shape>
    </item>

    <item
        android:bottom="1dp"
        android:left="0dp"
        android:right="1dp"
        android:top="0dp">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval">
            <gradient
                android:angle="0"
                android:endColor="@color/accent_end"
                android:startColor="@color/accent_start"
                android:type="linear" />
            <size
                android:width="@dimen/floating_circle_button_size"
                android:height="@dimen/floating_circle_button_size" />
            <padding
                android:bottom="0dip"
                android:left="0dip"
                android:right="0dip"
                android:top="0dip" />
        </shape>
    </item>
</layer-list>